<aura:component implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction" access="global" >
<aura:attribute type="Opportunity" name="opps" />
    <aura:registerEvent name="cmpEvent" type="c:eventonchild"/>
	<tr>
        <lightning:button label="{!v.opps.Name}" name="submit" onclick="{!c.oppclick}" /> <td></td>
        <td>{!v.opps.AccountId}</td>
        <td>{!v.opps.Type}</td>
        <td>{!v.opps.Probability}</td>
    </tr>
</aura:component>